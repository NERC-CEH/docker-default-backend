{
  "name": "datalabs-default",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "start": "nodemon --exec babel-node src/server.js",
    "clean": "shx rm -rf ./dist",
    "build": "babel src --ignore spec.js,__mocks__ --out-dir dist",
    "dist": "yarn clean && yarn build && yarn update-version",
    "test": "yarn test-ci -- --watch",
    "test-ci": "jest",
    "update-version": "VERSION=`git describe --tags --always` && sed -i -- 's/undefined/\"'\"$VERSION\"'\"/g' dist/version.js"
  },
  "dependencies": {
    "chalk": "2.3.0",
    "convict": "4.0.0",
    "express": "4.15.5",
    "pug": "2.0.0-beta11",
    "serve-favicon": "2.4.5",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "4.5.0",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "nodemon": "1.11.0",
    "shx": "0.2.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
